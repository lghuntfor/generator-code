SELECT TABLE_NAME, COLUMN_NAME ,DATA_TYPE, COLUMN_TYPE AS FULL_DATA_TYPE, COLUMN_DEFAULT, (IF(IS_NULLABLE='YES', TRUE, FALSE)) AS NULLABLE, (IF(COLUMN_KEY='PRI', TRUE, FALSE)) AS `primary`, COLUMN_COMMENT FROM information_schema.COLUMNS WHERE TABLE_NAME IN (${tableNames})
@if(isNotEmpty(dbName)) {
AND TABLE_SCHEMA = '${dbName}'
@}